{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceRoot": "src",
    "outDir": "dist",
    "lib": [
      "es6",
      "es2015"
    ],
    "sourceMap": true,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules",
      "./src/types"
    ],
    "types": [
      "oauth",
      "node",
      "form-data",
      "mocha",
      "request",
      "dotenv",
      "chai"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}