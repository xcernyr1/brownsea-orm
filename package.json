{
  "name": "brownsea-orm",
  "version": "0.0.1",
  "description": "An Object Mapper for the scouts.org API",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "pretest": "npm run build",
    "test": "npm run test:spec && npm run test:e2e",
    "build:tsc": "rm -rf dist && ntsc",
    "build": "npm run build:tsc",
    "postinstall": "typings install && npm prune && npm run build",
    "reinstall": "npm cache clean && npm install",
    "test:spec": "mocha ./dist/**/*.spec.js",
    "test:e2e": "mocha ./dist/**/*.e2e.js",
    "clean": "rm -rf ./dist"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "mocha": "2.5.3",
    "ntypescript": "1.201606150105.1",
    "oauth": "^0.9.14",
    "typings": "^1.2.0"
  },
  "keywords": [
    "scouts",
    "orm"
  ],
  "author": "probinson@nextfaze.com",
  "license": "MIT",
  "dependencies": {
    "chance": "^1.0.4",
    "connect-ensure-login": "^0.1.1",
    "dotenv": "^2.0.0",
    "loopback-component-passport": "https://github.com/NextFaze/loopback-component-passport.git",
    "passport-drupal": "https://github.com:NextFaze/passport-drupal.git",
    "querystring": "^0.2.0",
    "request": "^2.73.0"
  }
}
